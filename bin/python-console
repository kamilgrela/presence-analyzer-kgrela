#!/opt/python/2.7/bin/python

import sys

sys.path[0:0] = [
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/src',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/Paste-1.7.5.1-py2.7.egg',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/setuptools-3.4.4-py2.7.egg',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/PasteScript-1.7.5-py2.7.egg',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/PasteDeploy-1.5.2-py2.7.egg',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/Flask-0.10.1-py2.7.egg',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/itsdangerous-0.24-py2.7.egg',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/Jinja2-2.7.2-py2.7.egg',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/Werkzeug-0.9.4-py2.7.egg',
  '/home/kamil/Dokumenty/presence-analyzer-kgrela/eggs/MarkupSafe-0.21-py2.7-linux-x86_64.egg',
  ]


_interactive = True
if len(sys.argv) > 1:
    _options, _args = __import__("getopt").getopt(sys.argv[1:], 'ic:m:')
    _interactive = False
    for (_opt, _val) in _options:
        if _opt == '-i':
            _interactive = True
        elif _opt == '-c':
            exec(_val)
        elif _opt == '-m':
            sys.argv[1:] = _args
            _args = []
            __import__("runpy").run_module(
                 _val, {}, "__main__", alter_sys=True)

    if _args:
        sys.argv[:] = _args
        __file__ = _args[0]
        del _options, _args
        __file__f = open(__file__)
        exec(compile(__file__f.read(), __file__, "exec"))
        __file__f.close(); del __file__f

if _interactive:
    del _interactive
    __import__("code").interact(banner="", local=globals())
